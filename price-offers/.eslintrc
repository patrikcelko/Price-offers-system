{
    "extends": [
        "airbnb-typescript"
    ],
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "rules": {
        "react/jsx-props-no-spreading": "off",
        "linebreak-style": 0,
        "react/require-default-props": "off",
        "@typescript-eslint/no-unused-vars": "warn",
        "react/jsx-boolean-value": "off"
    }
}